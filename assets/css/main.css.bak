.navigation ul li a span{ margin:12px 0 0 13px;}

.navigation ul li a:hover {
    background: #232f40;
    color: #FFFFFF;
}
.navigation ul .active a {
    background: #232f40; color:#FFF;
	}
	
.navigation ul  li a img{ margin-left:10px;}

