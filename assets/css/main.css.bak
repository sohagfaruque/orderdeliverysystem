.bx .icon{ float:left; width:82px; height:77px; text-align:center;}

.green{ background:#6ccac9;}
.orange{ background:#ff6c60; }
.yellow{ background:#f8d347;}
.blue{ background:#57c8f2 ;}

/*
			Load CSS before JavaScript
		*/
		
		/*
			Slides container
			Important:
			Set the width of your slides container
			Set to display none, prevents content flash
		*/
		#slides .slides_container {
			width:570px;
			height:170px;
			display:none;
		}

		/*
			Each slide
			Important:
			Set the width of your slides
			If height not specified height will be set by the slide content
			Set to display block
		*/
		
		#slides .slides_container .contentdiv {
			width:570px;
			height:170px;
			display:block;
		}
		
		/*
			Slides container
			Important:
			Set the width of your slides container
			Set to display none, prevents content flash
		*/
		#slides_two .slides_container {
			width:250px;
			display:none;
		}

		/*
			Each slide
			Important:
			Set the width of your slides
			If height not specified height will be set by the slide content
			Set to display block
		*/
		
		#slides_two .slides_container .contentdiv {
			width:250px;
			height:250px;
			display:block;
		}
		

		
		/* 
			Example only
		*/
		.pagination .current a {
			color:red;
		}
		hr {
			background:#efefef;
		}
		
		
.pagination ul{ float:left; width:100%; margin:30px 0px 0px 0px;}
.pagination ul li{ float:left; width:auto;}
.menu_name{ float:left; width:auto; max-width:188px; background:#232f40; color:#FFF; border-radius:4px 4px 0px 0px; -webkit-border-radius:4px 4px 0px 0px; -moz-border-radius:4px 4px 0px 0px; font-family:'Open Sans', sans-serif; font-size:18px; padding:10px 20px 10px 20px;}

.menu_name:hover{ background:#424f63; color:#FFF;}
.pagination ul li .selected .menu_name{ background:#424f63;}

.contentdiv table{ width:100%;}
.contentdiv table th{ background:#dddddd; color:#232f40; font-family:'Open Sans', sans-serif;}
.contentdiv table td{ text-align:center; font-family:'Open Sans', sans-serif; }


