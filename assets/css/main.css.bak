.form .small-field{ float:left; width:48px; margin:22px 0px 0px 0px; -webkit-border-radius: 4px; -moz-border-radius: 4px; border-radius: 4px; min-height:48px; background:#dddddd;
border:none;}
.result{float:left; width:auto; margin:36px 5px 0px 6px;}
.button{ float:left; width:100%; max-width:331px; height:48px; border-bottom:solid 3px #101926; background:#232f40;-webkit-border-radius: 4px;
-moz-border-radius: 4px;
border-radius: 4px;; margin-top:10px; padding-left:11px;  margin:22px 28px 0;}
.button a{ float:left; text-align:center; font-family:Verdana, Geneva, sans-serif; font-size:18px; color:#FFF; margin:12px 0 18px 125px; text-transform:uppercase; font-family: 'Open Sans', sans-serif; text-decoration:none;}
.Remember span{ float:left;font-family:Verdana, Geneva, sans-serif; font-size:14px; color:#000;}
.left_side{ float:left; max-width:139px;}
.Remember{ float:left; margin:71px 9px 10px 9px;}
