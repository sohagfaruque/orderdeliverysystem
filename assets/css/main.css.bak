.logo{ text-align:center; margin:55px 0px 0px 0px ; width:auto;}
.box{ float:left; width:100%;text-align:center; margin:10px 0px 0px 0px; height:auto; background:#FFF;-moz-box-shadow: 0 0 5px #888;
-webkit-box-shadow: 0 0 5px#888;
box-shadow: 0 0 5px #888; width:100%;}
.form { float:left; width:100%; margin:19px 0px 0px 0px;}
